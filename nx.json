{
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"affected": {
		"defaultBase": "main"
	},
	"npmScope": "nxls",
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"workspaceLayout": {
		"appsDir": "packages",
		"libsDir": "packages"
	},
	"projects": {
		"wiki-bell": {
			"tags": []
		},
		"wiki-bell-ui": {
			"tags": []
		}
	}
}
